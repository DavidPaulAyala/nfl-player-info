<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="/css/styles.css">
    <title>Tight Ends</title>
  </head>
  <body>
    <div class="grass">
    <div class="container">
      <div class="jumbotron">
        <h1>Tight Ends</h1>
      </div>
        <div class="navbar navbar-default navbar-custom" role="navigation">
            <div class="container">
                <div class="navbar-header">
                  <h2>Top 15 Tight Ends for Week {{ week }}, {{ year }}</h2>
                  <a class="navbar-brand" href="/">Home</a>
                </div>
                <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Position<b class="caret"></b></a>
                        <ul class="dropdown-menu multi-level">
                            <li><a href="/qb/{{ week }}/{{ year }}">Quarterback</a></li>
                            <li><a href="/rb/{{ week }}/{{ year }}">Running Back</a></li>
                            <li><a href="/wr/{{ week }}/{{ year }}">Wide Receiver</a></li>
                            <li><a href="/te/{{ week }}/{{ year }}">Tight End</a></li>
                            <li><a href="/k/{{ week }}/{{ year }}">Kicker</a></li>
                            <li><a href="/def/{{ week }}/{{ year }}">Defense</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Year<b class="caret"></b></a>
                        <ul class="dropdown-menu multi-level">
                          <li><a href="/{{ pos }}/{{ week }}/2016">2016</a></li>
                          <li><a href="/{{ pos }}/{{ week }}/2015">2015</a></li>
                          <li><a href="/{{ pos }}/{{ week }}/2014">2014</a></li>
                          <li><a href="/{{ pos }}/{{ week }}/2013">2013</a></li>
                          <li><a href="/{{ pos }}/{{ week }}/2012">2012</a></li>
                          <li><a href="/{{ pos }}/{{ week }}/2011">2011</a></li>
                          <li><a href="/{{ pos }}/{{ week }}/2010">2010</a></li>
                          <li><a href="/{{ pos }}/{{ week }}/2009">2009</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Week<b class="caret"></b></a>
                        <ul class="dropdown-menu multi-level">
                          <li><a href="/{{ pos }}/1/{{ year }}">Week 1</a></li>
                          <li><a href="/{{ pos }}/2/{{ year }}">Week 2</a></li>
                          <li><a href="/{{ pos }}/3/{{ year }}">Week 3</a></li>
                          <li><a href="/{{ pos }}/4/{{ year }}">Week 4</a></li>
                          <li><a href="/{{ pos }}/5/{{ year }}">Week 5</a></li>
                          <li><a href="/{{ pos }}/6/{{ year }}">Week 6</a></li>
                          <li><a href="/{{ pos }}/7/{{ year }}">Week 7</a></li>
                          <li><a href="/{{ pos }}/8/{{ year }}">Week 8</a></li>
                          <li><a href="/{{ pos }}/9/{{ year }}">Week 9</a></li>
                          <li><a href="/{{ pos }}/10/{{ year }}">Week 10</a></li>
                          <li><a href="/{{ pos }}/11/{{ year }}">Week 11</a></li>
                          <li><a href="/{{ pos }}/12/{{ year }}">Week 12</a></li>
                          <li><a href="/{{ pos }}/13/{{ year }}">Week 13</a></li>
                          <li><a href="/{{ pos }}/14/{{ year }}">Week 14</a></li>
                          <li><a href="/{{ pos }}/15/{{ year }}">Week 15</a></li>
                          <li><a href="/{{ pos }}/16/{{ year }}">Week 16</a></li>
                        </ul>
                    </li>
                  </ul>
                </div>
            </div>
        </div>

      <table>
        <tr>
            <th>Name</th>
            <th>Team</th>
            <th>FF Points</th>
            <th>Rec Yards</th>
            <th>Rec TDs</th>
            <th>Fumbles</th>
        </tr>
        {% for player in players %}
            <tr>
                <td>{{ player.getFirstName }} {{ player.getLastName}}</td>
                <td>{{ player.getTeam }}</td>
                <td>{{ player.getFfpoints}}</td>
                <td>{{ player.getRecYds}}</td>
                <td>{{ player.getRecTds}}</td>
                <td>{{ player.getFum}}</td>
            </tr>
            {% endfor %}
            </table>
        </div>
        </div>
    </div>
  </body>
</html>
